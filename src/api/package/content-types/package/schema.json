{
  "kind": "collectionType",
  "collectionName": "packages",
  "info": {
    "singularName": "package",
    "pluralName": "packages",
    "displayName": "Package",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal"
    },
    "duration": {
      "type": "string"
    },
    "bookingPeriod": {
      "type": "date"
    },
    "travelPeriod": {
      "type": "date"
    },
    "provider": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::provider.provider"
    },
    "countries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::country.country"
    },
    "inclusions": {
      "type": "component",
      "repeatable": true,
      "component": "shared.list"
    },
    "exclusions": {
      "type": "component",
      "repeatable": true,
      "component": "shared.list"
    },
    "optionalTours": {
      "type": "component",
      "repeatable": true,
      "component": "shared.list"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
